<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/pay"
        android:layout_width="121dp"
        android:layout_height="39dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        android:backgroundTint="@color/gray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/to"
        android:layout_width="121dp"
        android:layout_height="40dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toEndOf="@+id/pay"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/balance"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pay" />

    <FrameLayout
        android:id="@+id/balance2"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="48dp"
        android:background="@color/gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/balance"
        app:layout_constraintTop_toBottomOf="@+id/to" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/cant"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toStartOf="@+id/cant2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balance" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/cant2"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balance2" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/rest"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cant" />

    <FrameLayout
        android:id="@+id/rest2"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginTop="76dp"
        android:background="@color/gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toEndOf="@+id/rest"
        app:layout_constraintTop_toBottomOf="@+id/cant2" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/frameLayout5"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rest2" />

    <FrameLayout
        android:background="@color/gray"
        android:id="@+id/frameLayout6"
        android:layout_width="251dp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout5" />

    <FrameLayout
        android:background="@color/gray"
        android:layout_width="251dp"
        android:layout_height="42dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout6" />



</androidx.constraintlayout.widget.ConstraintLayout>
